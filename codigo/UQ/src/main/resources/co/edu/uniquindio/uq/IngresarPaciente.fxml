<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>

<StackPane prefHeight="432.0" prefWidth="638.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.uniquindio.uq.viewController.IngresarPacienteController">
<AnchorPane prefHeight="400.0" prefWidth="552.0">
    <children>
        <TableView fx:id="tblPacientes" layoutX="193.0" layoutY="-5.0" prefHeight="432.0" prefWidth="476.0">
            <columns>
                <TableColumn fx:id="colNombre" prefWidth="115.0" text="Nombre" />
                <TableColumn fx:id="colCedula" prefWidth="118.0" text="Cédula" />
                <TableColumn fx:id="colDireccion" prefWidth="96.0" text="Dirección" />
                <TableColumn fx:id="colTelefono" prefWidth="148.0" text="Teléfono" />
            </columns>
        </TableView>
        <TextField fx:id="txtNombre" layoutX="5.0" layoutY="66.0" promptText="Nombre" />
        <TextField fx:id="txtCedula" layoutX="5.0" layoutY="99.0" promptText="Cédula" />
        <TextField fx:id="txtDireccion" layoutX="5.0" layoutY="133.0" promptText="Dirección" />
        <TextField fx:id="txtTelefono" layoutX="5.0" layoutY="167.0" promptText="Teléfono" />
        <Button layoutX="10.0" layoutY="20.0" onAction="#onActualizar" text="Actualizar" />
         <Button fx:id="btnVolver" layoutX="19.0" layoutY="393.0" mnemonicParsing="false" onAction="#setBtnVolver" text="Volver" />
         <Button fx:id="btnHistorial" layoutX="11.0" layoutY="241.0" mnemonicParsing="false" onAction="#onConsultarHistoria" text="Consultar Historial " />
         <Button fx:id="btnSolicitar" layoutX="13.0" layoutY="273.0" mnemonicParsing="false" onAction="#onSolicitudCita" text="Solicitar Cita" />
         <Button fx:id="btnCancelar" layoutX="11.0" layoutY="306.0" mnemonicParsing="false" onAction="#onCancelarCita" text="Cancelar Cita" />
    </children>
</AnchorPane>
</StackPane>
