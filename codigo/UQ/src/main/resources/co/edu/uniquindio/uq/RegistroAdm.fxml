<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="600.0" prefWidth="770.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.uniquindio.uq.viewController.RegistroAdmController">
    <children>
        <Label layoutX="108.0" layoutY="183.0" text="Nombre" />
        <Label layoutX="111.0" layoutY="243.0" text="Cédula" />
        <Label layoutX="111.0" layoutY="301.0" text="Dirección" />
        <Label layoutX="114.0" layoutY="360.0" text="Teléfono" />
        <Label layoutX="568.0" layoutY="151.0" text="Historial Médico" />


        <TextField fx:id="txtNombre" layoutX="205.0" layoutY="179.0" promptText="Nombre" />
        <TextField fx:id="txtCedula" layoutX="205.0" layoutY="239.0" promptText="Cédula" />
        <TextField fx:id="txtDireccion" layoutX="205.0" layoutY="297.0" promptText="Dirección" />
        <TextField fx:id="txtTelefono" layoutX="205.0" layoutY="356.0" promptText="Teléfono" />


        <TextArea fx:id="txtHistorial" layoutX="468.0" layoutY="192.0" prefHeight="188.0" prefWidth="277.0" promptText="Ingrese el Historial Médico" />


        <Button fx:id="btnGuardar" layoutX="393.0" layoutY="435.0" mnemonicParsing="false" onAction="#onGuardar" text="Guardar" />
        <Button fx:id="btnVolver" layoutX="671.0" layoutY="535.0" mnemonicParsing="false" onAction="#onVolver" text="Volver" />
    </children>
</AnchorPane>
