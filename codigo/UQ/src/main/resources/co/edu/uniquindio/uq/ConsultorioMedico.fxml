<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Text?>

<StackPane xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.uniquindio.uq.viewController.ConsultorioMedicoController">
    <AnchorPane prefHeight="450.0" prefWidth="638.0">
        <children>
            <Text layoutX="177.0" layoutY="82.0" style="-fx-font-size: 20px; -fx-padding: 10px;" text="Bienvenido al Consultorio Médico" />

            <Button fx:id="btnHistorial" layoutX="262.0" layoutY="142.0" mnemonicParsing="false" onAction="#onConsultarHistoria" text="Consultar Historial" />
            <Button fx:id="btnRegistrarDiagnostico" layoutX="254.0" layoutY="259.0" onAction="#onRegistrarDiagnostico" text="Registrar Diagnóstico" />
            <Button fx:id="btnAdministrarHorario" layoutX="256.0" layoutY="200.0" onAction="#onAdministrarHorario" text="Administrar Horarios" />
            <Button fx:id="btnNotificaciones" layoutX="231.0" layoutY="315.0" onAction="#onVerNotificaciones" prefHeight="25.0" prefWidth="190.0" text="Ver Notificaciones de Citas" />

            <Button fx:id="btnVolver" ellipsisString="" layoutX="35.0" layoutY="391.0" onAction="#onVolverAlLogin" prefHeight="27.0" prefWidth="108.0" text="Volver " />
        </children>
    </AnchorPane>
</StackPane>
